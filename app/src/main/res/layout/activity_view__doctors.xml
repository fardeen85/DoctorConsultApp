<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View_Doctors">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerviewdoctor"
        android:layout_width="350dp"
        android:layout_height="290dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.989" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="331dp"
        android:layout_height="146dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        app:cardCornerRadius="5dp"
        app:cardElevation="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.081">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/chipdentist"
                android:layout_width="39dp"
                android:layout_height="43dp"
                android:layout_above="@+id/textView18"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="274dp"
                android:layout_marginBottom="2dp"
                app:srcCompat="@drawable/tooth" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="261dp"
                android:layout_marginBottom="73dp"
                android:text="Dentist"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView45"
                android:layout_width="97dp"
                android:layout_height="22dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="184dp"
                android:layout_marginBottom="8dp"
                android:text="VETERINARY"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="73dp"
                android:layout_marginBottom="10dp"
                android:text="COUNSELLOR"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView41"
                android:layout_width="101dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="136dp"
                android:layout_marginBottom="75dp"
                android:text="Gastroenete"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView44"
                android:layout_width="101dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="9dp"
                android:layout_marginBottom="66dp"
                android:text="GENERAL PRACTITIONER"
                android:textAlignment="center"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/chipg"
                android:layout_width="47dp"
                android:layout_height="39dp"
                android:layout_above="@+id/textView41"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="168dp"
                android:layout_marginBottom="0dp"
                app:srcCompat="@drawable/gas" />

            <ImageView
                android:id="@+id/chipv"
                android:layout_width="45dp"
                android:layout_height="38dp"
                android:layout_above="@+id/textView45"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="211dp"
                android:layout_marginBottom="3dp"
                app:srcCompat="@drawable/vertinery" />

            <ImageView
                android:id="@+id/chipgp"
                android:layout_width="52dp"
                android:layout_height="37dp"
                android:layout_above="@+id/textView44"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="33dp"
                android:layout_marginBottom="-3dp"
                app:srcCompat="@drawable/general" />

            <ImageView
                android:id="@+id/chipc"
                android:layout_width="46dp"
                android:layout_height="36dp"
                android:layout_above="@+id/textView46"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="96dp"
                android:layout_marginBottom="0dp"
                app:srcCompat="@drawable/counsell" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView47"
        android:layout_width="118dp"
        android:layout_height="26dp"
        android:layout_marginTop="12dp"
        android:text="Catogories"
        android:textAlignment="textStart"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>